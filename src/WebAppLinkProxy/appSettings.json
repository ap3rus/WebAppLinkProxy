{
    "routingConfig": {
        "domains": {
            "www.youtube.com": {
                "description": "myTube",
                "patterns": [ "/watch\\?v=(?<video>[^&]*)", "/p/(?<video>[^\\?]*)", "/v/(?<video>[^\\?]*)" ],
                "launchUri": "vnd.youtube:{video}"
            },
            "youtube.com": {
                "description": "myTube",
                "patterns": [ "/watch\\?v=(?<video>[^&]*)", "/p/(?<video>[^\\?]*)", "/v/(?<video>[^\\?]*)" ],
                "launchUri": "vnd.youtube:{video}"
            },
            "m.youtube.com": {
                "description": "myTube",
                "patterns": [ "/watch\\?v=(?<video>[^&]*)" ],
                "launchUri": "vnd.youtube:{video}"
            },
            "youtu.be": {
                "description": "myTube",
                "patterns": [ "/(?<video>[^\\?]*)" ],
                "launchUri": "vnd.youtube:{video}"
            }
        }
    }
}